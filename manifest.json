{
  "name": "Snack'n'Track",
  "short_name": "S&T",
 "start_url": "/",
  "background_color": "#f77f00",
  "theme_color": "#f77f00",
  "orientation": "any",
  "description": "Snack'n'Track vous aide à suivre vos envies de snacks et votre humeur, sans prise de tête.",
  "categories": ["productivity", "health"],
  "lang": "fr",
  "id": "/index.html",
  "scope": "/",
  "display": "standalone",
  "dir": "ltr",
  "display_override": ["standalone", "browser"],
  "launch_handler": {
  "client_mode": "auto"
},
  "file_handlers": [
   {
    "action": "/open.html",  
    "accept": {
      "text/csv": [".csv"]
    }
  }
  ],

"screenshots": [
  {
    "src": "/screenshot/screenshot1.png",
    "sizes": "1280x720",
    "type": "image/png",
    "form_factor": "wide"
  },
  {
    "src": "/screenshot/screenshot2.png",
    "sizes": "640x1136",
    "type": "image/png",
    "form_factor": "narrow"
  }
],
   "share_target": {
    "action": "/.netlify/functions/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "file",
          "accept": ["image/*", "text/csv"]
        }
      ]
    }
  },

 "shortcuts": [
    {
      "name": "Ajouter un snack",
      "short_name": "Ajout snack",
      "description": "Accéder rapidement à l'ajout de snack",
      "url": "/index.html",
      "icons": [
        {
          "src": "/assets/manifest-icon-192.maskable.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    },
    {
      "name": "Voir mes humeurs",
      "short_name": "Mes humeurs",
      "description": "Accéder à mes humeurs",
      "url": "/mes-humeurs.html",
      "icons": [
        {
          "src": "/assets/manifest-icon-192.maskable.png",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ],
"icons": [
  {
    "src": "./assets/manifest-icon-192.maskable.png",
    "sizes": "192x192",
    "type": "image/png",
    "purpose": "maskable"
  },
  {
    "src": "./assets/manifest-icon-512.maskable.png",
    "sizes": "512x512",
    "type": "image/png",
    "purpose": "maskable"
  },
  {
    "src": "/assets/manifest-icon-192.png",
    "sizes": "192x192",
    "type": "image/png",
    "purpose": "any"
  },
  {
    "src": "/assets/manifest-icon-512.png",
    "sizes": "512x512",
    "type": "image/png",
    "purpose": "any"
  }
]
}