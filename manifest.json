{
  "name": "Snack'n'Track",
  "short_name": "S&T",
  "start_url": "./index.html",
  "background_color": "#f77f00",
  "theme_color": "#f77f00",
  "orientation": "portrait-primary",
  "description": "Snack'n'Track vous aide à suivre vos envies de snacks et votre humeur, sans prise de tête.",
  "lang": "fr",
  "id": "./index.html",
  "scope": "./",
  "display": "standalone",
  "file_handlers": [
    {
      "action": "/open",
      "accept": {
        "text/csv": [
          ".csv"
        ]
      }
    }
  ],
  "share_target": {
    "action": "/share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "files": [
        {
          "name": "file",
          "accept": [
            "image/*",
            "text/csv"
          ]
        }
      ]
    }
  },
  "categories": [
    "productivity",
    "lifestyle"
  ],

   "shortcuts": [
    {
      "name": "Ajouter un snack",
      "short_name": "Ajout snack",
      "description": "Accéder rapidement à l'ajout de snack",
      "url": "/ajouter-snack",
      "icons": [
        {
          "src": "assets/manifest-icon-192.maskable.png",
          "sizes": "192x192"
        }
      ]
    },
    {
      "name": "Voir mes humeurs",
    "url": "/mes-humeurs.html",
    "icons": [{
      "src": "assets/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png"
        }
      ]
    }
  ],
  "icons": [
    {
      "src": "assets/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "assets/manifest-icon-192.maskable.png",
      "sizes": "192x192",
      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "assets/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "assets/manifest-icon-512.maskable.png",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ]
}